<div class="post-cats post-card__cats">
    <% (post.categories || []).forEach(function(item) { %>
        <a href="<%- url_for(item.path) %>" class="post-cats__link button"><%= item.name %></a>
    <% }); %>
    <% (post.tags || []).forEach(function(item) { %>
        <a href="<%- url_for(item.path) %>" class="post-tags__link button"># <%= item.name %></a>
    <% }); %>
</div>